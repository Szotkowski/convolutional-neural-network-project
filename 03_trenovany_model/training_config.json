{
  "dataset_dir": "dataset",
  "epochs": 100,
  "batch_size": 8,
  "grad_accum_steps": 1,
  "lr": 1e-05,
  "lr_encoder": 1e-06,
  "weight_decay": 0.0001,
  "num_workers": 4,
  "device": "cuda",
  "output_dir": "output",
  "early_stopping": true,
  "early_stopping_patience": 5,
