  "warmup_epochs": 2,
  "lr_drop": 33,
  "clip_max_norm": 0.1,
  "use_ema": true,
  "ema_decay": 0.995,
  "use_augment": false,
  "mixup_alpha": 0.0,
  "dropout_rate": 0.1
}